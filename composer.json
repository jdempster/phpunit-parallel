{
    "name": "alexdempster44/phpunit-parallel",
    "description": "Run PHPUnit tests in parallel with a beautiful terminal UI",
    "type": "composer-plugin",
    "license": "Unlicense",
    "require": {
        "php": ">=7.4",
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "PhpunitParallel\\Composer\\": "composer/"
        }
    },
    "extra": {
        "class": "PhpunitParallel\\Composer\\Plugin"
    },
    "bin": ["composer/bin/phpunit-parallel"]
}
